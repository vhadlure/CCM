@model Clinic.Models.ClinicModel.ClinicModel
<div class="box-header with-border">
    <h3 class="box-title"></h3>
</div>

<form class="form-horizontal" id="ClinicMasterFormID" action="/Clinic/SaveClinicDetails" method="post">
    @Html.HiddenFor(model => model.ClinicID)
    <div class="box-body">
        <div class="form-group">
            <label for="" class="col-sm-2 control-label">Name<span class="red"> *</span></label>
            <div class="col-sm-3">
                @Html.TextBoxFor(model => model.Name, new { @class = "form-control ", placeholder = "Name" })
            </div>
            <label for="" class="col-sm-2 control-label">Address<span class="red"> *</span></label>
            <div class="col-sm-3">
                @Html.TextAreaFor(model => model.Address, new { @class = "form-control ", placeholder = "Address" })
            </div>
        </div>
    </div>
    <div class="box-footer">
        <div class="pull-right">
            <button type="button" class="btn btn-success" onclick="SaveClientDetails();">Save</button>
            <button type="button" class="btn btn-primary btn-flat" onclick="ResetClientDetails();">Reset</button>
        </div>
    </div>
</form>
<script src="~/js/ClientMaster.js" type="text/javascript"></script>

