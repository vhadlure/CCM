@model Clinic.Models.UserModel.UserModel

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="content-header">
    <h1>
        User
    </h1>
    <ol class="breadcrumb">
        <li><a href="/Home/Dashboard"><i class="fa fa-dashboard"></i>Home</a></li>
        <li><a href="">User</a></li>
    </ol>
</section>

<section class="content">
    <div class="box box-primary">
        <div class="col-md-3 pull-right">
            <div class="input-group margin">
                <input type="text" class="form-control" id="filter">
                <span class="input-group-btn">
                    <button class="btn btn-primary btn-flat" type="button" title="Type to search"><i class="fa fa-fw fa-search"></i></button>
                </span>
            </div>
        </div>
        <button type="button" class="btn btn-primary btn-flat pull-right" onclick="AddNewUser();" style="margin: 8px 0px 0px 10px;">New</button>
        <div class="clearfix"></div>
        <div class="box-body">
            <table id="userListGrid"></table>
        </div>
    </div>
</section>
<script src="~/js/UserList.js"></script>